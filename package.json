{
  "name": "linter-ruff",
  "linter-package": true,
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Linting Python files using ruff",
  "activationEvents": [],
  "homepage": "https://github.com/bacadra/atom-linter-ruff",
  "url": "https://github.com/bacadra/atom-linter-ruff",
  "repository": "https://github.com/bacadra/atom-linter-ruff",
  "license": "MIT",
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "activationHooks": [
    "language-python:grammar-used",
    "MagicPython:grammar-used",
    "django-atom:grammar-used",
    "atom-django:grammar-used"
  ],
  "dependencies": {
    "tmp": "^0.2.1"
  }
}
