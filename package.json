{
  "name": "linter-ruff",
  "description": "A wrapper around Python linter called ruff",
  "repository": "https://github.com/bacadra/pulsar-linter-ruff",
  "keywords": [
    "linter",
    "python",
    "ruff",
    "pyflakes",
    "pycodestyle",
    "mccabe",
    "isort",
    "pep8",
    "pyupgrade",
    "flake8",
    "pylint",
    "numpy"
  ],
  "version": "1.1.1",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationHooks": [
    "language-python:grammar-used",
    "MagicPython:grammar-used",
    "django-atom:grammar-used",
    "atom-django:grammar-used"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "tmp": "^0.2.1"
  },
  "linter-package": true
}
